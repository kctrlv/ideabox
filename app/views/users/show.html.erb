<h1> Welcome, <%= @user.username %>! </h1>

<h3> Your Ideas: </h3>
<ul>
  <% @user.ideas.each do |idea| %>
    <li>
      <%= link_to(idea.title, user_idea_path(idea)) %>
      <%= link_to("Edit", edit_user_idea_path(idea)) %>
      <%= link_to("Delete", user_idea_path(idea), method: :delete) %>
    </li>

  <% end %>
</ul>

<%= link_to "New Idea", new_user_idea_path(current_user.id) %>
