<h1> Welcome, <%= @user.username %>! </h1>

<h3> Your Ideas: </h3>
<ul>
  <% @user.ideas.each do |idea| %>
    <li>
      <%= link_to(idea.name, user_idea_path(idea)) %>
      <%= link_to("Edit", edit_user_idea_path(idea)) %>
      <%= link_to("Delete", user_idea_path(idea), method: :delete) %>
    </li>

  <% end %>
</ul>
